{
	"name": "@obelism/react-svg",
	"description": "A performant way to load and show SVG in React applications",
	"keywords": [
		"svg",
		"react",
		"icons",
		"request",
		"cache"
	],
	"license": "MIT",
	"version": "2.1.2",
	"author": "Obelism <hello@obelism.studio> (https://www.obelism.studio)",
	"repository": {
		"type": "git",
		"url": "https://github.com/Obelism/react-svg"
	},
	"types": "./dist/index.d.ts",
	"main": "./dist/index.cjs",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "bunchee ./src/index.ts -w",
		"build": "bunchee ./src/index.ts",
		"format": "prettier --write --ignore-unknown .",
		"preview": "vite preview",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage",
		"test:ci": "vitest run"
	},
	"peerDependencies": {
		"react": ">=17.0.0",
		"swr": "^2.2.4"
	},
	"devDependencies": {
		"@testing-library/react": "^14.0.0",
		"@types/node": "^20.8.7",
		"@types/react": "^18.2.31",
		"@vitejs/plugin-react": "^4.1.0",
		"@vitest/coverage-c8": "^0.33.0",
		"bunchee": "^3.1.1",
		"eslint": "^8.51.0",
		"jsdom": "^22.0.0",
		"prettier": "^3.0.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"ts-node": "^10.9.1",
		"tslib": "^2.6.2",
		"typescript": "^5.0.4",
		"vitest": "^0.34.6"
	}
}
