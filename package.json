{
	"name": "@obelism/react-svg",
	"description": "A performant way to load and show SVG in React applications",
	"keywords": [
		"svg",
		"react",
		"icons",
		"request",
		"cache"
	],
	"license": "MIT",
	"version": "1.1.3",
	"author": "Obelism <hello@obelism.studio> (https://www.obelism.studio)",
	"repository": {
		"type": "git",
		"url": "https://github.com/Obelism/react-svg"
	},
	"types": "./dist/react-svg.d.ts",
	"main": "./dist/react-svg.esm.js",
	"module": "./dist/react-svg.esm.js",
	"exports": {
		"require": "./dist/react-svg.cjs",
		"import": "./dist/react-svg.mjs",
		"module": "./dist/react-svg.esm.js"
	},
	"scripts": {
		"dev": "bunchee ./src/main.ts -w",
		"build": "bunchee ./src/main.ts",
		"format": "prettier '**' --write",
		"preview": "vite preview",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage",
		"test:ci": "vitest run"
	},
	"files": [
		"dist"
	],
	"peerDependencies": {
		"react": ">=17.0.0",
		"swr": ">=1.0.0"
	},
	"devDependencies": {
		"@testing-library/react": "^13.4.0",
		"@types/node": "^18.11.18",
		"@types/react": "^18.0.27",
		"@vitejs/plugin-react": "^3.1.0",
		"@vitest/coverage-c8": "^0.28.4",
		"bunchee": "^2.2.0",
		"eslint": "^8.33.0",
		"jsdom": "^21.1.0",
		"prettier": "^2.8.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"swr": "^2.0.3",
		"ts-node": "^10.9.1",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vitest": "^0.28.4"
	}
}
